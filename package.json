    "jimp": "^0.16.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "6",
    "responsive-loader-sharp": "^0.8.0",
    "workbox-background-sync": "^6.5.3",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-routing": "^6.5.3",
